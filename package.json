    "build": "gulp && eleventy",
    "start": "gulp && concurrently \"gulp watch\" \"eleventy --serve\"",
    "debug": "DEBUG=* eleventy",
    "format:": "prettier '**/*.js' --write && eslint '**/*.js' --fix",
    "lint": "eslint --ext .js"
  },
  "author": {
    "name": "Christina Dan",
    "email": "hello@christinadan.com",
    "url": "https://christinadan.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/christinadan/christinadan-11ty"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "concurrently": "^5.3.0",
    "del": "^5.1.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-flatmap": "^1.0.2",
    "gulp-header": "^2.0.9",
    "gulp-imagemin": "^7.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-svgmin": "^3.0.0",
    "gulp-terser": "^1.4.0",
    "html-minifier": "^4.0.0",
    "husky": "^4.3.0",
    "lazypipe": "^1.0.2",
    "lint-staged": "^10.4.1",
    "markdown-it": "^11.0.1",
    "markdown-it-anchor": "^6.0.0",
    "markdown-it-attrs": "^3.0.3",
    "markdown-it-link-attributes": "^3.0.0",
    "merge2": "^1.4.1",
    "prettier": "^2.1.2",
    "sass": "^1.27.0",
    "sharp": "^0.26.2",
    "terser": "^5.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "eslint"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@11ty/eleventy-img": "^0.3.0",
    "slugify": "^1.4.5"
  }
